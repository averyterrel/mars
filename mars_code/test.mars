module test;

import mem "./memory";
import neptune "./neptune";

main :: #section("boot") fn() {
    
    str  := "bruh";
    str2 := []u8{0x62, 0x72, 0x75, 0x68};

    is_eq := mem.equal(str.base, str2.base, str.len);

    if is_eq {
        neptune.kprint("equal");
    } else {
        neptune.kprint("not equal");
    }
}