module test;

import mem "memory";
import neptune "neptune";

let println = neptune::kprintln;
let printf = neptune::kprintf;

// #block_symbol sets the symbol of a function's code
// block to a specific string
let main = fn #block_symbol("main") () {

    let str = "bruh";
    let str2 = "bruh2";

    if mem::str_eq(str, str2) {
        println("equal");
    } else {
        println("not equal");
    }

    for i: u64 in 0..<10 {
        printf("%int\n", {&i});
    }

    while true {}
};